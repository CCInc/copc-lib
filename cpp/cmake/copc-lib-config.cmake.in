@PACKAGE_INIT@

# Include the exported CMake file
get_filename_component(copclib_CMAKE_DIR "${CMAKE_CURRENT_LIST_FILE}" PATH)

# This macro enables usage of find_dependency().
# https://cmake.org/cmake/help/v3.11/module/CMakeFindDependencyMacro.html
include(CMakeFindDependencyMacro)

# Declare the used packages in order to communicate the requirements upstream.
find_dependency(lazperf 2.1.0 REQUIRED)

if(NOT TARGET COPCLIB::copc-lib)
    include("${copc-lib_CMAKE_DIR}/copc-lib-targets.cmake")
endif()

check_required_components(copclib)